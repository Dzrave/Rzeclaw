{
  "name": "rzeclaw-terminal",
  "version": "0.1.0",
  "description": "Rzeclaw desktop terminal (Channel) â€” Windows",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win"
  },
  "build": {
    "appId": "com.rzeclaw.terminal",
    "productName": "Rzeclaw Terminal",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "renderer.js"
    ],
    "win": {
      "target": [
        "portable",
        "nsis"
      ]
    }
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "bonjour": "^3.5.1"
  }
}
